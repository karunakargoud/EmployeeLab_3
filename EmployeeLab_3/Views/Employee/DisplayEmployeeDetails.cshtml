@using EmployeeLab_3.Models
@model Employee
@{
    ViewBag.Title = "DisplayEmployeeDetails";
}

<h2>Display Employee Details</h2>
<table>
    <tr>
        <th>Employee Id</th>
        <td>@Model.EmployeeId</td>
    </tr>
    <tr>
        <th>Employee Name</th>
        <td>@Model.EmployeeName</td>
    </tr>
    <tr>
        <th>Designation</th>
        <td>@Model.Designation</td>
    </tr>
    <tr>
        <th>age</th>
        <td>@Model.age </td>
    </tr>
    <tr>
        <th>Salary</th>
        <td>@Model.Salary</td>
    </tr>
</table>

